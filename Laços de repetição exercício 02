/*
2. Faça um programa que exiba na tela a tabuada do número 5 no seguinte formato: 5X1=5;
5X2=10; 5X3=15; ... ; 5X10=50
*/

#include <iostream>
using namespace std;

int main ()
{
    int cont = 1, mult = 5;
    
    while (cont<=10)
    {
        cout << mult << "x" << cont << "=" << mult*cont << endl;
        cont ++;
    }
    
    return 0;
    
}
