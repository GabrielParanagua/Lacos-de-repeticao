/*
5. Faça um programa que exiba na tela a soma dos números inteiros do intervalo [100, 200].
Exemplo: soma = 100 + 101 +102 + ... + 200.
*/

#include <iostream>
using namespace std;

int main ()
{
    
    int cont = 100;
    int soma = 0;
    
    while(cont<=200)
    {
        soma += cont;
        cont++;
    }
    
    cout << soma;
    
    return 0;
}
